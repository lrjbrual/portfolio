<div class="wrapper" id="aboutRj">
    <div class="page-header page-header-xs banner-image-about" data-parallax="true">
      <h3 class="typeBanner">
        <span id="banner-about-text"></span>
      </h3>
        <%# <div class="filter"></div> %>
    </div>
    <div class="section profile-content">
        <div class="container">
            <div class="owner">
                <div class="avatar">
                    <%= image_tag "faces/meandyou.jpeg", alt:"Circle Image", class:"img-circle img-no-padding img-responsive" %>
                </div>
                <div class="name">
                    <h4 class="note"><%= @ownerTitle %><br /></h4>
                    <span class="label label-default"><%= @ownerWork1 %></span>
                    <span class="label label-warning"><%= @ownerWork2 %></span>
                    <span class="label label-primary"><%= @ownerWork3 %></span>
                    <span class="label label-success"><%= @ownerWork4 %></span>
                    <span class="label label-info"><%= @ownerWork5 %></span>
                </div>
            </div>
                <div class="row">
                    <div class="col-md-6 ml-auto mr-auto text-center">
                        <p>RyanJeff</p>
                        <script src="//platform.linkedin.com/in.js" type="text/javascript"></script>
                        <script type="IN/MemberProfile" data-id="http://www.linkedin.com/in/lrjbrual" data-format="hover" data-related="false"></script>
                        <br />
                        <%= render 'partials/emailbutton'%>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>	
<%= render :file => 'pages/new' %>

